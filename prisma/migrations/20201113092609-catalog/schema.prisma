// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Request {
    id Int @default(autoincrement()) @id
    slug String @unique
    userData Json
    requestInfo Json?
    updatedAt DateTime @default(now())
    createdAt DateTime @default(now())
}

model Catalog {
    id Int @default(autoincrement()) @id
    slug String @unique
    name String
    category String
    brand String
    distributor String
    source String
    price Float
    description String
    images Json
    updatedAt DateTime @default(now())
    createdAt DateTime @default(now())
}
